FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      kt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "../system/runConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{
    foil
    {
        type            fixedValue;
        value           uniform 0.0;
    }
    outer
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    "(front|back)"
    {
    	type empty;
	}
}


